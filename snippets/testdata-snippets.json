  "Basic TestData Template": {
    "prefix": "testdata-basic",
    "body": [
      "{",
      "  \"\\$schema\": \"./schemas/testDataProtocolFile.schema.json\",",
      "  \"SimulatedData\": [",
      "    {",
      "      \"TrackNumber\": \"${1:3}\",",
      "      \"Counter\": ${2:0},",
      "      \"MainErrorCode\": ${3:0},",
      "      \"PlacedLabwareItems\": [",
      "        {",
      "          \"Barcode\": \"${4:Sample01}\",",
      "          \"LabwareId\": \"${5:301}\",",
      "          \"PositionId\": \"${6:1}\",",
      "          \"MainErrorCode\": ${7:0}",
      "        }",
      "      ]",
      "    }",
      "  ]",
      "}"
    ],
    "description": "Basic TestData template for simulation"
  },

